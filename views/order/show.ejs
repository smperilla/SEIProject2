<div class="get-fortune">

    <h2>Fortune for <%=order.userId.name %>
    </h2>
    <h4>Total: <%order.total%>
    </h4>

    <% order.fortunes.forEach(fortune=> {%>
        <div>

            <p>
                <%=fortune.name %> - <%=fortune.likelihood %>
            </p>
            <p>
                <%=fortune.description%>
            </p>
        </div>



        <% }) %>

</div>